  name=\"name\"\r\n                  placeholder=\"Camisa de manga corta\"\r\n                  invalid={errors.name ? true : false}\r\n                  innerRef={register}\r\n                />\r\n                <CInvalidFeedback>{errors.name?.message}</CInvalidFeedback>\r\n              </CFormGroup>\r\n              <CFormGroup>\r\n                <CLabel>Descripción</CLabel>\r\n                <Controller\r\n                  name=\"description\"\r\n                  control={control}\r\n                  render={({ onChange }) => (\r\n                    <ReactQuill\r\n                      className={errors.description && \"is-invalid\"}\r\n                      theme=\"snow\"\r\n                      onChange={onChange}\r\n                      modules={quillConfig.modules}\r\n                      formats={quillConfig.formats}\r\n                    />\r\n                  )}\r\n                />\r\n                <CInvalidFeedback>\r\n                  {errors.description?.message}\r\n                </CInvalidFeedback>\r\n              </CFormGroup>\r\n            </CCardBody>\r\n          </CCard>\r\n\r\n          <CCard>\r\n            <CCardHeader>\r\n              <strong>Precios</strong>\r\n            </CCardHeader>\r\n            <CCardBody>\r\n              <CFormGroup row>\r\n                <CCol md=\"6\">\r\n                  <CLabel>Precio</CLabel>\r\n                  <CInputGroup className={errors.price ? \"is-invalid\" : null}>\r\n                    <CInputGroupPrepend>\r\n                      <CInputGroupText>S/.</CInputGroupText>\r\n                    </CInputGroupPrepend>\r\n                    <CInput\r\n                      type=\"number\"\r\n                      name=\"price\"\r\n                      placeholder=\"0.00\"\r\n                      invalid={errors.price ? true : false}\r\n                      innerRef={register}\r\n                    />\r\n                  </CInputGroup>\r\n                  <CInvalidFeedback>{errors.price?.message}</CInvalidFeedback>\r\n                </CCol>\r\n                <CCol md=\"6\">\r\n                  <CLabel>Precio de comparación</CLabel>\r\n                  <CInputGroup\r\n                    className={errors.compareAtPrice ? \"is-invalid\" : null}\r\n                  >\r\n                    <CInputGroupPrepend>\r\n                      <CInputGroupText>S/.</CInputGroupText>\r\n                    </CInputGroupPrepend>\r\n                    <CInput\r\n                      type=\"number\"\r\n                      name=\"compareAtPrice\"\r\n                      placeholder=\"0.00\"\r\n                      invalid={errors.compareAtPrice ? true : false}\r\n                      innerRef={register}\r\n                    />\r\n                    <CInputGroupAppend>\r\n                      <CInputGroupText>\r\n                        <CTooltip\r\n                          content=\"Para mostrar un precio rebajado, mueve el precio original del producto al precio de comparación. Introduce un valor menor para el precio.\"\r\n                          placement=\"bottom\"\r\n                        >\r\n                          <CIcon name=\"cil-info\" />\r\n                        </CTooltip>\r\n                      </CInputGroupText>\r\n                    </CInputGroupAppend>\r\n                  </CInputGroup>\r\n                  <CInvalidFeedback>\r\n                    {errors.compareAtPrice?.message}\r\n                  </CInvalidFeedback>\r\n                </CCol>\r\n              </CFormGroup>\r\n              <hr />\r\n              <CFormGroup row>\r\n                <CCol md=\"6\">\r\n                  <CLabel>Costo por artículo</CLabel>\r\n                  <CInputGroup\r\n                    className={errors.unitCost ? \"is-invalid\" : null}\r\n                  >\r\n                    <CInputGroupPrepend>\r\n                      <CInputGroupText>S/.</CInputGroupText>\r\n                    </CInputGroupPrepend>\r\n                    <CInput\r\n                      type=\"number\"\r\n                      name=\"unitCost\"\r\n                      placeholder=\"0.00\"\r\n                      invalid={errors.unitCost ? true : false}\r\n                      innerRef={register}\r\n                    />\r\n                  </CInputGroup>\r\n                  <div className=\"help-block\">\r\n                    <small>Los clientes no verán esta información.</small>\r\n                  </div>\r\n                  <CInvalidFeedback>\r\n                    {errors.unitCost?.message}\r\n                  </CInvalidFeedback>\r\n                </CCol>\r\n                <CCol md=\"3\">\r\n                  <CLabel>Margen</CLabel>\r\n                  <CInput name=\"margin\" innerRef={register} disabled />\r\n                </CCol>\r\n                <CCol md=\"3\">\r\n                  <CLabel>Ganancia</CLabel>\r\n                  <CInput name=\"profit\" innerRef={register} disabled />\r\n                </CCol>\r\n              </CFormGroup>\r\n              <CFormGroup variant=\"custom-checkbox\">\r\n                <CInputCheckbox\r\n                  custom\r\n                  id=\"Taxable\"\r\n                  name=\"taxable\"\r\n                  innerRef={register}\r\n                />\r\n                <CLabel variant=\"custom-checkbox\" htmlFor=\"Taxable\">\r\n                  Cobrar impuesto sobre la venta de este producto\r\n                </CLabel>\r\n              </CFormGroup>\r\n            </CCardBody>\r\n          </CCard>\r\n\r\n          <CCard>\r\n            <CCardHeader>\r\n              <strong>Estado del producto</strong>\r\n            </CCardHeader>\r\n            <CCardBody>\r\n              <CSelect>\r\n                <option>Borrador</option>\r\n                <option>Público</option>\r\n              </CSelect>\r\n            </CCardBody>\r\n            <CCardFooter>\r\n              <CButton color=\"primary\" type=\"submit\">\r\n                Enviar\r\n              </CButton>\r\n            </CCardFooter>\r\n          </CCard>\r\n        </CCol>\r\n        <CCol xs=\"12\" md=\"4\">\r\n          <CCard>\r\n            <CCardHeader>\r\n              <strong>Errores de validación</strong>\r\n            </CCardHeader>\r\n            <CCardBody>\r\n              <pre>{JSON.stringify(errors, null, 2)}</pre>\r\n            </CCardBody>\r\n          </CCard>\r\n          <CCard>\r\n            <CCardHeader>\r\n              <strong>Datos validos</strong>\r\n            </CCardHeader>\r\n            <CCardBody>\r\n              <pre>{JSON.stringify(getValues(), null, 2)}</pre>\r\n            </CCardBody>\r\n          </CCard>\r\n        </CCol>\r\n      </CRow>\r\n    </CForm>\r\n  );\r\n};\r\n\r\nexport default NewCustomer;\r\n","C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\views\\Forms\\Forms.js",[],"C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\views\\Forms\\New.js",[],"C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\utils\\form.js",[],"C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\views\\Forms\\Attrs\\index.js",[],"C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\views\\Forms\\Attrs\\PlaceholderAttr.js",[],"C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\views\\Forms\\Attrs\\RowsAttr.js",[],"C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\views\\Forms\\Attrs\\RequiredAttr.js",[],"C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\views\\Forms\\Attrs\\ReadonlyAttr.js",[],"C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\utils\\slug.js",[],"C:\\xampp\\htdocs\\nativos-digitales\\okr\\coreui-base\\src\\utils\\camelCase.js",[],{"ruleId":"235","replacedBy":"236"},{"ruleId":"237","replacedBy":"238"},{"ruleId":"239","severity":1,"message":"240","line":36,"column":7,"nodeType":"241","messageId":"242","endLine":36,"endColumn":36},"no-native-reassign",["243"],"no-negated-in-lhs",["244"],"no-te